{
  "apps": {
    "http": {
      "servers": {
        "coder": {
          "listen": [":443"],
          "routes": [
            {
              "match": [
                {
                  "host": ["hworld.lab.nslab.kristianjones.dev"]
                }
              ],
              "handle": [
                {
                  "handler": "subroute",
                  "routes": [
                    {
                      "handle": [
                        {
                          "handler": "vars",
                          "root": "/var/www/html"
                        },
                        {
                          "handler": "file_server",
                          "browse": {
                            "template_file": ""
                          }
                        }
                      ]
                    }
                  ]
                }
              ]
            }
          ],
          "experimental_http3": true
        }
      }
    },
    "tls": {
      "certificates": {
        "load_folders": ["/SSL"]
      }
    }
  }
}
