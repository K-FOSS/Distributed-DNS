{
  "name": "web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build": "ts-node --project bin/tsconfig.json bin/build",
    "dev": "ts-node --project bin/tsconfig.json bin/watch",
    "gen": "graphql-codegen --config graphql-codegen.yml",
    "prettier": "prettier -c ./UI/**/*.ts* ",
    "lint": "eslint --ext tsx,ts UI Server bin"
  },
  "browserslist": [
    "last 1 Chrome version"
  ],
  "alias": {
    "UI": "./UI",
    "Server": "./Server"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "arrowParens": "always",
    "semi": true,
    "jsxSingleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "react-hooks",
      "@typescript-eslint"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "project": "./tsconfig.json",
      "tsconfigRootDir": ".",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "standard",
      "plugin:@typescript-eslint/recommended",
      "plugin:react/recommended",
      "prettier",
      "prettier/standard",
      "prettier/@typescript-eslint",
      "prettier/react"
    ],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "rules": {
      "react-hooks/rules-of-hooks": "error",
      "react-hooks/exhaustive-deps": "error",
      "react/jsx-uses-react": "error",
      "react/jsx-uses-vars": "error",
      "@typescript-eslint/camelcase": "warn",
      "no-unused-vars": "off",
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "react/no-children-prop": "off",
      "react/display-name": "off",
      "react/prop-types": "off",
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "args": "none",
          "varsIgnorePattern": "_.+"
        }
      ],
      "@typescript-eslint/no-var-requires": "warn",
      "@typescript-eslint/no-object-literal-type-assertion": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "off",
      "@typescript-eslint/no-non-null-assertion": "off",
      "@typescript-eslint/explicit-member-accessibility": "off"
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-ssr": "^3.1.3",
    "@koa/router": "^8.0.2",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.33",
    "@material-ui/styles": "^4.6.0",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "apollo-utilities": "^1.3.2",
    "clsx": "^1.0.4",
    "date-fns": "^2.8.1",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-tag": "^2.10.1",
    "isomorphic-unfetch": "^3.0.0",
    "koa": "^2.11.0",
    "koa-static": "^5.0.0",
    "material-table": "^1.54.0",
    "module-alias": "^2.2.2",
    "node-forge": "^0.9.1",
    "notistack": "^0.9.6",
    "react": "^16.12.0",
    "react-cookie": "^4.0.1",
    "react-dom": "^16.12.0",
    "react-hook-form": "^3.28.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-ssr-prepass": "^1.0.8",
    "react-syntax-highlighter": "^11.0.2",
    "refractor": "^2.10.0",
    "universal-cookie-koa": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@graphql-codegen/cli": "1.8.3",
    "@graphql-codegen/fragment-matcher": "1.8.3",
    "@graphql-codegen/near-operation-file-preset": "1.8.3",
    "@graphql-codegen/schema-ast": "1.8.3",
    "@graphql-codegen/typescript": "1.8.3",
    "@graphql-codegen/typescript-document-nodes": "1.8.3",
    "@graphql-codegen/typescript-graphql-files-modules": "1.8.3",
    "@graphql-codegen/typescript-operations": "1.8.3",
    "@graphql-codegen/typescript-react-apollo": "1.8.3",
    "@types/fs-extra": "8.0.1",
    "@types/koa": "2.0.52",
    "@types/koa-static": "4.0.1",
    "@types/koa__router": "8.0.2",
    "@types/module-alias": "2.0.0",
    "@types/node": "12.12.32",
    "@types/node-forge": "0.9.0",
    "@types/parcel-bundler": "1.12.1",
    "@types/react": "16.9.27",
    "@types/react-dom": "16.9.4",
    "@types/react-router": "5.1.4",
    "@types/react-router-dom": "5.1.3",
    "@types/react-syntax-highlighter": "11.0.2",
    "@types/sharp": "0.23.0",
    "@types/workbox-sw": "4.3.0",
    "@typescript-eslint/eslint-plugin": "2.8.0",
    "@typescript-eslint/parser": "2.8.0",
    "babel-plugin-module-resolver": "3.2.0",
    "chokidar": "3.3.0",
    "eslint": "6.7.0",
    "eslint-config-prettier": "6.7.0",
    "eslint-config-standard": "14.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.4",
    "eslint-plugin-node": "10.0.0",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-standard": "4.0.1",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-assets-list": "1.7.1",
    "parcel-plugin-bundle-manifest": "0.2.0",
    "parcel-plugin-compress": "1.1.0",
    "prettier": "1.19.1",
    "sharp": "0.23.3",
    "to-ico": "1.1.5",
    "ts-node": "8.5.2",
    "typescript": "3.7.2"
  }
}
